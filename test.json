{
  "$id": "https://example.com/person.schema.json",
  "required": ["name", "age"],
  "properties": {
    "name": {
      "type": "string",
      "default": "Nguyen Anh Tu"
    },
    "age": {
      "type": "number",
      "default": 21
    },
    "male": {
      "type": "boolean",
      "default": true
    },
    "test": {
      "type": "object",
      "require": ["test2"],
      "properties": {
        "test1": {
          "type": "number",
          "default": 12,
          "minimum": 10,
          "maximum": 20
        },
        "test2": {
          "type": "string"
        }
      }
    },
    "parents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "default": "asdf"
          },
          "age": {
            "type": "number"
          }
        }
      }
    },
    "st": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "number"
        }
      }
    }
  }
}
